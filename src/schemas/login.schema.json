{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1655849800.json", 
	"title": "Root", 
	"type": "object",
	"required": [
		"username",
		"password"
	],
	"properties": {
		"username": {
			"$id": "#root/username", 
			"title": "Username", 
			"type": "string",
			"default": "",
			"examples": [
				"t"
			],
			"pattern": "^[a-z0-9_-]{3,50}$"
		},
		"password": {
			"$id": "#root/password", 
			"title": "Password", 
			"type": "string",
			"default": "",
			"examples": [
				"Test!235"
			],
			"pattern": "^(?=.*?[A-Z])(?=.*?[a-z])(?=.*?[0-9])(?=.*?[#?!@$ %^&*-]).{8,100}$"
		}
	}
}
